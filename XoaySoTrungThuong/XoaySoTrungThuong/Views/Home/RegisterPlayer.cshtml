@{
    ViewBag.Title = "Đăng ký nhân viên";
}

<br />
<h2>@ViewBag.Title.</h2>


<h4>Import danh sách nhân viên</h4>
@{
    if (Request.QueryString["status"] == "1")
    {
        <h4 style="color:red;">Thêm danh sách nhân viên thành công</h4>
    }
}
@using (Html.BeginForm("RegisterPlayerUpload", "Home", FormMethod.Post, new { EncType = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group">
        <p><b>Upload file Excel nhân viên</b></p>
        <div class="col-md-10">
            <input type="file" name="file" />
            <br />
            <input type="submit" class="btn btn-default" value="Register" />
        </div>
        <p>

        </p>
    </div>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
